{
    "name": "Delayed Tab",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "A Mailbox App style 'read later' function for browser tabs. This extension was created with the awesome extensionizr.io",
    "homepage_url": "http://extensionizr.com",
    "icons": {
        "16" : "icons/icon16.png",
        "48" : "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "js/lodash.min.js",
            "js/moment.min.js",
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_icon" : "icons/icon19.png",
        "default_title": "Delayed tabs list",
        "default_popup": "html/browser_action.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "alarms",
        "background"
    ]
}