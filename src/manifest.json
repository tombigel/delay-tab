{
    "name": "Delay Tab",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "A Mailbox App inspired 'read later' function for browser tabs. This extension was created with the awesome extensionizr.io",
    "homepage_url": "https://github.com/tombigel/delay-tab",
    "icons": {
        "16" : "icons/icon16.png",
        "48" : "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "scripts": [
            "libs/lodash.min.js",
            "libs/moment.min.js",
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_icon" : "icons/icon19.png",
        "default_title": "Delayed tabs list",
        "default_popup": "html/browser_action.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "alarms",
        "background"
    ]
}